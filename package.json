{
  "name": "rydzynskim.github.io",
  "description": "Personal Website",
  "author": "Mitchel Rydzynski",
  "private": true,
  "scripts": {
    "compile": "tsc && npx browserify src/index.ts -v --debug -p tsify -o dist/index.js",
    "format": "npx prettier --write src/*.ts",
    "lint": "tsc && npx eslint . --ext .ts",
    "local-dev": "mkdir -p dist && npx watchify src/index.ts -v --debug -p tsify -o dist/index.js"
  },
  "dependencies": {
    "canvas": "^2.8.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.2.0",
    "http-status-codes": "^2.1.4",
    "node-fetch": "^2.6.6",
    "q": "^1.5.1",
    "react": "^18.2.0",
    "react-addons-css-transition-group": "^15.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.14.25",
    "@types/node-fetch": "^2.5.12",
    "@types/q": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "browserify": "^17.0.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "mocha": "^9.1.1",
    "nyc": "^15.0.1",
    "prettier": "2.7.1",
    "source-map-support": "^0.5.19",
    "tsify": "^5.0.4",
    "typescript": "^4.4.2",
    "watchify": "^4.0.0"
  }
}
